<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_intro"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kr.studiows.ohmock.introActivity">

    <kr.studiows.ohmock.DefaultButton
        android:id="@+id/BTN_pvp"
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_marginTop="217dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="PvP" />

    <kr.studiows.ohmock.DefaultButton
        android:id="@+id/BTN_vsCpu"
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/BTN_pvp"
        android:layout_centerHorizontal="true" />

    <kr.studiows.ohmock.DefaultButton
        android:id="@+id/BTN_setting"
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/BTN_vsCpu"
        android:layout_alignLeft="@+id/BTN_vsCpu"
        android:layout_alignStart="@+id/BTN_vsCpu" />

    <TextView
        android:layout_height="wrap_content"
        android:id="@+id/Title"
        android:text="OhMock-!"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="87dp"
        android:layout_width="wrap_content"
        android:textSize="36sp" />
</RelativeLayout>
